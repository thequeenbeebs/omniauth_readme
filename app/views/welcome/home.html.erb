<% if session[:user_id] %> 
    <h1><%= @user.name %></h1> 
    <h2>Email: <%= @user.email %></h2> 
    <h2>Facebook UID: <%= @user.uid %></h2> 
    <img src="https://raw.githubusercontent.com/learn-co-curriculum/omniauth_readme/master/&lt;%= @user.image %&gt;"> 
<% else %> 
    <%=link_to('Log in with Facebook!', '/auth/facebook') %>
<% end %>